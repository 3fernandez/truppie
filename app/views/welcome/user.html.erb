<%= render 'layouts/navbar' %>
<div id='intro'>
  <div class='cta container'>
    <img alt='Splash' class='splash' src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/203277/adventure-splash.svg'>
    <h1 class="emblem-font">Explore e experimente</h1>
    <p><%= t('user-adventure-info') %></p>
    <%= link_to new_user_session_path, class: 'button' do %>
        <span>Comece a se aventurar</span>
    <% end %>
  </div>
</div>
<div id='tours'>
