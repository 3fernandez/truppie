<%= render 'layouts/flash' %>
<div class="container">
	<div class="row">
		<h2 class="spaced-default">
			Criação de Webhooks
		</h2>
		<div class="col-xs-12">
			<% if !@webhook_id.nil? %>
			<h4 class="spaced-smallest">ID do webhook criado</h4>
			<p class="spaced-down">
				<span class="label label-pill label-primary display-1"><%= @webhook_id  %></span>
			</p>
			<% end %>
			<% if !@webhook_return_url.nil? %>
			<h4 class="spaced-smallest">URL do Webhook</h4>
			<p class="spaced-down">
				<span class="label label-pill label-primary display-1"><%= @webhook_return_url %></span>
			</p>
			<% end %>			
		</div>
	</div>
</div>
