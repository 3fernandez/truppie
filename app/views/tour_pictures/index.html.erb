<p id="notice"><%= notice %></p>

<h1>Listing Tour Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Photo</th>
      <th>Tour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tour_pictures.each do |tour_picture| %>
      <tr>
        <td><%= tour_picture.photo %></td>
        <td><%= tour_picture.tour %></td>
        <td><%= link_to 'Show', tour_picture %></td>
        <td><%= link_to 'Edit', edit_tour_picture_path(tour_picture) %></td>
        <td><%= link_to 'Destroy', tour_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tour picture', new_tour_picture_path %>
